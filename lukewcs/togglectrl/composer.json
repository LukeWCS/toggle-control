{
    "name": "lukewcs/togglectrl",
    "type": "phpbb-extension",
    "description": "Makes it possible to decide centrally for all extensions which visual style should be used for yes/no switches, provided extensions support Toggle Control.",
    "homepage": "https://github.com/LukeWCS/toggle-control",
    "version": "1.0.0-b3",
    "time": "2023-12-04",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "LukeWCS",
            "homepage": "https://github.com/LukeWCS",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0,<8.4.0@dev",
        "composer/installers": "~1.0.0"
    },
    "extra": {
        "display-name": "Toggle Control",
        "lang-min-ver": "1.0.0",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.11,<3.4.0@dev"
        }
    }
}
